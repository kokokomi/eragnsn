; 石投げ
@enemy_command_1(enemyNum, charactorNum)
  #DIM enemyNum
  #DIM charactorNum

  CALL charactor_damage_hp(charactorNum, calculate_hp_damage_to_player(charactorNum, enemyNum, 100))

@enemy_command_1_message(enemyNum, charactorNum)
  #DIM enemyNum
  #DIM charactorNum
  RESULTS = %battle_enemy_name(enemyNum)%は%charactor_get_charactor_name(charactorNum)%に向かって石を投げた