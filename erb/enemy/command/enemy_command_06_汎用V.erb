@enemy_command_6(enemyNum, charactorNum)
#DIM enemyNum
#DIM charactorNum

IF CFLAG:(charactorNum):拘束 == enemyNum || CFLAG:(charactorNum):丸呑み == enemyNum
    CALL charactor_damage_ep(charactorNum, 150, part_build(false, false, true, false))
ELSE
    CALL charactor_damage_ep(charactorNum, 100, part_build(false, false, true, false))
ENDIF

@enemy_command_6_message(enemyNum, charactorNum)
#DIM enemyNum
#DIM charactorNum
IF CFLAG:(charactorNum):拘束 == enemyNum || CFLAG:(charactorNum):丸呑み == enemyNum
    RESULTS = %汎用技メッセージ_V_拘束(enemyNum, charactorNum)%
ELSE
    RESULTS = %汎用技メッセージ_V(enemyNum, charactorNum)%
ENDIF