@enemy_command_7(enemyNum, charactorNum)
#DIM enemyNum
#DIM charactorNum
IF CFLAG:(charactorNum):拘束 == enemyNum || CFLAG:(charactorNum):丸呑み == enemyNum
    CALL charactor_damage_ep(charactorNum, 150, part_build(false, false, true, false))
ELSE
    CALL charactor_damage_ep(charactorNum, 100, part_build(false, false, true, false))
ENDIF

@enemy_command_7_message(enemyNum, charactorNum)
#DIM enemyNum
#DIM charactorNum
IF CFLAG:(charactorNum):拘束 == enemyNum || CFLAG:(charactorNum):丸呑み == enemyNum
    RESULTS = %battle_enemy_name(enemyNum)%は%charactor_get_charactor_name(charactorNum)%の尻の入り口を刺激している！
ELSE
    RESULTS = %battle_enemy_name(enemyNum)%は%charactor_get_charactor_name(charactorNum)%の尻を撫で回している！
ENDIF