@汎用技メッセージ_C(enemyNum, charactorNum)
#FUNCTIONS
#DIM enemyNum
#DIM charactorNum

SELECTCASE battle_enemy_id(enemyNum)
; ヒルチャール系
CASE 1, 2, 3, 4, 5, 6, 7
    RETURNF @"%battle_enemy_name(enemyNum)%は%charactor_get_charactor_name(charactorNum)%のクリトリスを乱暴にこすっている！"
CASEELSE
    RETURNF @"%battle_enemy_name(enemyNum)%は%charactor_get_charactor_name(charactorNum)%の秘部を刺激している！"
ENDSELECT

