@汎用技メッセージ_A(enemyNum, charactorNum)
#FUNCTIONS
#DIM enemyNum
#DIM charactorNum

SELECTCASE battle_enemy_id(enemyNum)
; ヒルチャール系
CASE 1, 2, 3, 4, 5, 6, 7
    RETURNF @"%battle_enemy_name(enemyNum)%は%charactor_get_charactor_name(charactorNum)%の尻を撫で回している！"
; スライム系
CASE 50 TO 63
    RETURNF @"%battle_enemy_name(enemyNum)%は%charactor_get_charactor_name(charactorNum)%の尻を撫で回している！"
CASEELSE
    RETURNF @"%battle_enemy_name(enemyNum)%は%charactor_get_charactor_name(charactorNum)%の尻を撫で回している！"
ENDSELECT

@汎用技メッセージ_A_拘束(enemyNum, charactorNum)
#FUNCTIONS
#DIM enemyNum
#DIM charactorNum

SELECTCASE battle_enemy_id(enemyNum)
; ヒルチャール系
CASE 1, 2, 4
    RETURNF @"%battle_enemy_name(enemyNum)%は%charactor_get_charactor_name(charactorNum)%の腰を掴み、尻穴に男根を押し当てると一気に奥まで貫いた！"
; 大型ヒルチャール系
CASE 3, 5, 6, 7
    RETURNF @"%battle_enemy_name(enemyNum)%は%charactor_get_charactor_name(charactorNum)%の腰を掴み、尻穴に巨根を強引に突き入れた！"
; スライム系
CASE 50 TO 63
    RETURNF @"%battle_enemy_name(enemyNum)%は%charactor_get_charactor_name(charactorNum)%の尻穴にスライムが入り込む！"
; 人間系
CASE 200 TO 203
    RESULTS = @"%charactor_get_charactor_name(charactorNum)%を"
    SELECTCASE RAND:3
    CASE 0
        RESULTS += 四つん這いにさせると
    CASE 1
        RESULTS += 仰向けに押し倒すと
    CASE 2
        RESULTS += 自身の上に跨がらせると
    ENDSELECT
    RESULTS += @"%battle_enemy_name(enemyNum)%は男根を尻穴に挿入した！"
    RETURNF RESULTS
CASEELSE
    RETURNF @"%battle_enemy_name(enemyNum)%は%charactor_get_charactor_name(charactorNum)%の尻の入り口を刺激している！"
ENDSELECT