; 敵の名前
@enemy_name_1
#FUNCTIONS
RESULTS = ヒルチャール
RETURNF RESULTS

; HP
@enemy_max_hp_1(enemyLevel)
#FUNCTION
#DIM enemyLevel
RESULT = 1000 + (enemyLevel * 100)
RETURNF RESULT

; 行動選択
@enemy_select_command_1(enemyNum)
#DIM enemyNum
#DIM commandTarget
#DIM enemyCommands = 1, 2, 3, 5, 6

CALL enemy_strategy_random(enemyCommands)
RETURN RESULT:0, RESULT:1

; 攻撃力
@enemy_atk_1(enemyLevel)
#FUNCTION
#DIM enemyLevel
RESULT = 40 + (enemyLevel * 40)
RETURNF RESULT