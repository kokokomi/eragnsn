@喘ぎ声_快楽(level, length = 2)
  #FUNCTIONS
  #DIM level
  #DIM lcount
  #DIM length

  #DIMS 快楽ダメ1_parts = "くっ……ふぅっ", "あっ……", "はぁ、はぁ……", "んんっ……！"
  #DIMS 快楽ダメ2_parts = "はっ、はあっ……♡", "あ、ああぁッ……", "くっ、あぁぁっ", "あぁっ", "ううっ……！", "あぁっ……んんっ……！", "んっ……♡", "はぁ……♡ "
  #DIMS 快楽ダメ3_parts = "あ゛あ゛ぁ……♡", "お゛っ……♡", "はぁーっ♡ はぁーっ♡", "んぐぅっ！？"

  RESULTS = 
  FOR lcount, 0, length
    SELECTCASE level
      CASE 1
        RESULTS += array_get_random_element_str(快楽ダメ1_parts)
      CASE 2
        RESULTS += array_get_random_element_str(快楽ダメ2_parts)
      CASE 3
        RESULTS += array_get_random_element_str(快楽ダメ3_parts)
    ENDSELECT
  NEXT
  RETURNF RESULTS

