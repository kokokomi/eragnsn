; 元素スキル
@do_skill_10(charactorNum, targetEnemyNum)
  #DIM charactorNum
  #DIM targetEnemyNum

  CALL skill_util_attack(charactorNum, targetEnemyNum, 280)
  CALL increace_elemental_orb(true, targetEnemyNum, ELEMENT_TYPE_雷, 1)

  ; 元素爆発
@do_burst_10(charactorNum, targetEnemyNum)
  #DIM charactorNum
  #DIM targetEnemyNum

  CALL skill_util_attack(charactorNum, targetEnemyNum, 121)
  CALL increace_elemental_orb(true, targetEnemyNum, ELEMENT_TYPE_雷, 2)
  CALL skill_util_random_target_attack(charactorNum, 96, 0, 3)